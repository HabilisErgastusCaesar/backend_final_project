{
  "name": "express-bookings",
  "version": "1.0.0",
  "type": "module",
  "prisma": {
    "seed": "node ./prisma/seed.js"
  },
  "description": "Your REST API for managing bookings",
  "main": "src/index.js",
  "scripts": {
    "dev": "nodemon src/index.js && npm run sentry:sourcemaps",
    "test-positive": "newman run \"./postman/collections/Bookings API.json\" -e \"./postman/environments/Local.postman_environment.json\"",
    "test-negative": "newman run \"./postman/collections/Bookings API Negative.json\" -e \"./postman/environments/Local.postman_environment.json\"",
    "test": "npm run test-positive && npm run test-negative",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org bart-ea --project node-express node_modules && sentry-cli sourcemaps upload --org bart-ea --project node-express node_modules"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.4.1",
    "@sentry/cli": "^2.42.2",
    "@sentry/node": "^7.120.3",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "uuid": "^9.0.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "newman": "^6.0.0",
    "nodemon": "^3.0.1",
    "prisma": "^6.4.1"
  },
  "engines": {
    "node": ">=18 <19"
  }
}
